<?xml version="1.0" ?>
<!-- 
	Anticipatory Story Reader core properties
	tqos-asr-documizer extends tqos-asr-core so these properties
	serve far more than this agent
	Most of the properties here are just for booting ASRCore
	The primary properties used by this agent are:
		JSONFileBase
		BaseDataPath
-->

<properties>

	<parameter name="VocabServerURl" value="localhost" /> <!--  coordinate with VocabServer -->
	
	<!-- "T" means rewind kafka -->
	<parameter name="ConsumerRewind" value="F" /> 
	

	<list name="Parsers">
		<parameter name="LinkGrammar" value="org.topicquests.os.asr.driver.LGDriverEnvironment" /> 
		<parameter name="Eidos" value="org.topicquests.os.asr.driver.EDriverEnvironment" /> 
		<parameter name="Spacy" value="org.topicquests.os.asr.driver.sp.SpacyDriverEnvironment" /> 
		<parameter name="Indra" value="org.topicquests.os.asr.driver.InDriverEnvironment" /> 
	</list>

	
	
</properties>

